{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "rootDir": "src",
    "incremental": true,
    "module": "ES2015",
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "sourceMap": true,
  }
}